{
  "name": "govuk-express-template",
  "version": "1.0.0",
  "description": "description",
  "author": "Nikhil Nanjappa",
  "license": "ISC",
  "engines": {
    "node": "22.20.0"
  },
  "scripts": {
    "test:lint": "./node_modules/.bineslint .",
    "test:lint:fix": "eslint . --fix",
    "test:style": "prettier . --check",
    "test:style:fix": "prettier . --write",
    "test:security:audit": "npm audit || true",
    "test:security:lockfile": "lockfile-lint --path package-lock.json  --allowed-hosts nexus.nonprod.dwpcloud.uk registry.npmjs.org gitlab.com --validate-https",
    "test:security:outdated": "npm outdated || true",
    "test:security:all": "npm run test:security:lockfile && npm run test:security:outdated && npm run test:security:audit",
    "test:unit": "LOGLEVEL=fatal NODE_ENV= jest --coverage",
    "pipeline": "npm run test:lint:fix && npm run test:style:fix && npm run test:security:all && npm run test:unit",
    "test": "npm run pipeline",
    "build": "rm -rf dist && webpack --mode production",
    "start": "env $(cat .env.local | xargs) node ./src/server/index.js"
  },
  "commitlint": {
    "extends": [
      "@dwp/commitlint-config-base"
    ]
  },
  "nodemonConfig": {
    "ignore": [
      "*/__test__/*"
    ],
    "ext": "js,njk,html"
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text"
    ],
    "tempDirectory": "./coverage"
  },
  "dependencies": {
    "@babel/plugin-transform-runtime": "7.22.5",
    "@babel/preset-env": "7.22.5",
    "@babel/register": "7.21.0",
    "babel-loader": "8.3.0",
    "connect-redis": "7.1.1",
    "cookie-parser": "1.4.6",
    "cors": "2.8.5",
    "css-loader": "6.8.1",
    "csurf": "1.11.0",
    "eslint-webpack-plugin": "5.0.2",
    "express": "5.1.0",
    "express-session": "1.17.3",
    "express-validator": "7.0.1",
    "glob": "8.1.0",
    "govuk-frontend": "6.0.0-beta.0",
    "helmet": "6.2.0",
    "i18next": "25.6.2",
    "i18next-http-middleware": "3.8.2",
    "i18next-fs-backend": "2.6.1",
    "luxon": "3.3.0",
    "mini-css-extract-plugin": "2.7.6",
    "nunjucks": "3.2.4",
    "redis": "4.7.1",
    "sass-loader": "13.2.2",
    "serve-favicon": "2.5.0",
    "session-file-store": "1.5.0",
    "webpack": "5.103.0",
    "webpack-node-externals": "3.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.28.5",
    "@babel/eslint-parser": "7.28.4",
    "@babel/runtime": "7.26.10",
    "@commitlint/cli": "19.8.1",
    "@dwp/commitlint-config-base": "5.0.0",
    "@dwp/eslint-config-base": "9.0.2",
    "@dwp/eslint-config-mocha": "1.3.1",
    "@dwp/nyc-config-base": "1.1.1",
    "autoprefixer": "10.4.14",
    "dotenv": "17.2.3",
    "eslint": "9.37.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-import-resolver-webpack": "0.13.10",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-prettier": "5.5.4",
    "jest": "29.5.0",
    "lockfile-lint": "4.14.1",
    "nodemon": "2.0.22",
    "nyc": "17.1.0",
    "postcss": "8.5.6",
    "postcss-loader": "7.3.0",
    "postcss-preset-env": "^10.4.0",
    "prettier": "3.6.2",
    "sass": "1.62.1",
    "webpack-cli": "^5.1.4"
  }
}
